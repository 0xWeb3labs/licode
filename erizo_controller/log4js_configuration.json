{
  "appenders": [
    {
      "type": "file",
      "filename": "erizo_controller.log",
      "layout": {
        "type": "pattern",
        "pattern": "%d  - %p: %c - %m",
        "replaceConsole": true
      }
    }
  ],
  "levels": {
    "AMQPER": "ERROR",
    "Client": "INFO",
    "Connection": "INFO",
    "ErizoController": "INFO",
    "ErizoJS": "INFO",
    "ErizoJSController": "INFO",
    "ErizoAgent": "INFO",
    "ErizoAgentReporter": "ERROR",
    "EcCloudHandler": "INFO",
    "PerformanceStats": "INFO",
    "Publisher": "INFO",
    "Subscriber": "INFO",
    "Room": "INFO",
    "RoomController": "INFO",
    "RovClient":"INFO",
    "RovMetricsServer":"INFO",
    "RovReplManager":"INFO",
    "RPCPublic":"ERROR"
  }
}

